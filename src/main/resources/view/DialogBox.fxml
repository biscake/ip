<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="BOTTOM_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../css/dialog.css" type="javafx.scene.layout.HBox" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <VBox prefHeight="201.0" HBox.hgrow="ALWAYS">
         <children>
            <HBox fx:id="textBoxContainer" prefHeight="134.0" prefWidth="404.0" VBox.vgrow="ALWAYS">
               <children>
                  <Region prefHeight="115.0" prefWidth="50.0" />
                    <Label fx:id="dialog" alignment="TOP_LEFT" maxWidth="Infinity" minHeight="-Infinity" style="-fx-text-fill: #674544;" text="Label" textFill="#674544" wrapText="true">
                     <font>
                        <Font name="Microsoft Sans Serif" size="12.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets bottom="10.0" left="8.0" right="8.0" top="10.0" />
               </padding>
            </HBox>
            <HBox fx:id="imageContainer" prefHeight="100.0" prefWidth="200.0">
               <children>
                    <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" styleClass="profile-image" />
                  <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
